# Temporal.io Worker configuration options
# Change both of these to match your environment
TEMPORAL_HOST="127.0.0.1"
TEMPORAL_NAMESPACE="default"
TEMPORAL_TASKQUEUE="lotus-bot:ipc"
TEMPORAL_NOTIFICATION_CHAT_IDS_TELEGRAM=''
TEMPORAL_NOTIFICATION_CHAT_IDS_DISCORD=''
# The string used to split the command from message data
TEMPORAL_COMMAND_DELIM=""
# Comma-separated list of Telegram user IDs allowed to send commands
TEMPORAL_COMMAND_ADMINS=""
# Comman-separated list of commands allowed on this instance
TEMPORAL_COMMAND_ENABLED=""
# The type of workflow we are signaling with command data
TEMPORAL_COMMAND_WORKFLOW_TYPE=""
# The running Temporal workflow to signal with the command/data
TEMPORAL_COMMAND_WORKFLOW_ID=""
# The signal to send to the running workflow
# The workflow will process data type according to the signal sent
TEMPORAL_COMMAND_WORKFLOW_SIGNAL=""
